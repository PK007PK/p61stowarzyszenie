{
  "name": "gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby / sanity boilerplate",
  "author": "Piotr Krasny",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "start": "npm run develop",
    "serve": "cross-env NODE_OPTIONS=\"-r esm\" gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@styled-icons/fa-solid": "^10.34.0",
    "algoliasearch": "^4.10.4",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "escape-string-regexp": "^1.0.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "esm": "^3.2.25",
    "gatsby": "^3.7.2",
    "gatsby-background-image": "^1.5.3",
    "gatsby-plugin-algolia": "^0.22.1",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-gatsby-cloud": "^2.11.0",
    "gatsby-plugin-google-gtag": "^3.11.0",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-loadable-components-ssr": "^3.2.0",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-perf-budgets": "^0.0.18",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.24",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-source-sanity": "^7.0.7",
    "gatsby-transformer-sharp": "^3.7.1",
    "gsap": "^3.7.0",
    "hamburger-react": "^2.4.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-instantsearch-dom": "^6.12.1",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-share": "^4.4.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1"
  }
}
